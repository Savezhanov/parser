        <script>

        function checkPosition(){
            if(localStorage.getItem("landingBottomBlock") === 'show' && $(window).scrollTop() + $(window).height() === $(document).height()) {
                bottomBlock.css({position: 'relative', width: 'auto'});
            }else if ( $(window).scrollTop() + $(window).height() <= ($(document).height()-200) ){
                bottomBlock.css({position: 'fixed', width: '100%'});
            }
        }

        var bottomBlock = $("#landing-bottom-block");
        $(window).scroll(function() {
            checkPosition();
        });

//        if(!localStorage.getItem("landingBottomBlock")){
//            localStorage.setItem("landingBottomBlock", 'show');
//        }

        if(localStorage.getItem("landingBottomBlock") === 'show'){
            bottomBlock.removeClass('hideInBottom');
            checkPosition();
        }


        $(".closeLanding").on("click", function(){
            bottomBlock.addClass('hideInBottom');
            if (bottomBlock.css('relative') === 'relative') bottomBlock.remove();
            localStorage.setItem("landingBottomBlock", 'hide');
        });
    </script>

